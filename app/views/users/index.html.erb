<p id="notice"><%= notice %></p>

<h1><%=t(:'Users')%></h1>

<table>
  <thead>
    <tr>
      <th><%=t(:'Username')%></th>
      <th><%=t(:'Result')%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.result %></td>
        <td><%= link_to t(:'Show'), user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t(:'New User'), new_user_path %>
